{
  "name": "uic-design",
  "version": "1.1.3-alpha",
  "author": {
    "name": "rabbit",
    "email": "piotr.rgl@gmail.com"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run format"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-lib": "ng build uic-design",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --reporters --code-coverage --progress",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "stylelint": "npx stylelint ./projects/**/*.scss --fix",
    "lint": "tsc --noEmit && eslint . --ext ts --quiet --fix",
    "format": "prettier projects/**/*.ts --write"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^14.2.0",
