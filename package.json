    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@storybook/angular": "^6.5.12",
    "@types/color": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "browserslist": "^4.21.1",
